# Instagram Scraper Environment Variables
# Copy this file to .env and fill in your actual values

# Instagram credentials (required)
IGSCRAPER_SESSION_ID=your_session_id_here
IGSCRAPER_CSRF_TOKEN=your_csrf_token_here

# Optional configuration
IGSCRAPER_OUTPUT_DIR=./downloads
IGSCRAPER_CONCURRENT_DOWNLOADS=3
IGSCRAPER_REQUESTS_PER_MINUTE=60
IGSCRAPER_NOTIFICATIONS_ENABLED=true
IGSCRAPER_LOG_LEVEL=info

# Advanced settings
IGSCRAPER_USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36
IGSCRAPER_DOWNLOAD_TIMEOUT=30s
IGSCRAPER_RETRY_ATTEMPTS=3
IGSCRAPER_SKIP_VIDEOS=false
IGSCRAPER_CREATE_USER_FOLDERS=true